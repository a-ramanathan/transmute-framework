{
  "name": "transmute-framework",
  "version": "0.1.0",
  "description": "An ethereum smart contract framework.",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "clean": "rm -rf docs build",
    "docs-gen": "rm -rf docs; jsdoc --configure .jsdoc.json --verbose",
    "docs-serve": "http-server docs",
    "docs-debug": "rm -rf docs; jsdoc --configure .jsdoc.json --verbose; http-server docs;",
    "jshint": "jshint src",
    "jscs": "jscs src",
    "lint": "npm run jshint && npm run jscs",
    "testrpc": "pm2 start testrpc"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "mocha": "3.3.0",
    "webpack": "2.4.1",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transmute-industries/transmute-framework.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "security",
    "javascript",
    "truffle",
    "transmute"
  ],
  "contributors": [
    "Eric Olszewski <eolszewski@gmail.com>",
    "Orie Steele <orie@or13.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transmute-industries/transmute-framework/issues"
  },
  "homepage": "https://github.com/transmute-industries/transmute-framework#readme",
  "dependencies": {
    "truffle": "^3.2.1",
    "bluebird": "^3.4.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "ethereumjs-testrpc": "^3.0.5",
    "firebase": "^3.2.1",
    "firebase-tools": "^3.0.6",
    "http-server": "^0.9.0",
    "jscs": "^3.0.7",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.0"
  }
}