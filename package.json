{
  "name": "transmute-framework",
  "version": "0.1.0",
  "description": "An ethereum smart contract framework.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf docs build",
    "test": "mocha --recursive src ",
    "docs-gen": "rm -rf docs; jsdoc --configure .jsdoc.json --verbose",
    "docs-serve": "http-server docs",
    "docs-debug": "rm -rf docs; jsdoc --configure .jsdoc.json --verbose; http-server docs;",
    "jshint": "jshint src",
    "jscs": "jscs src",
    "lint": "npm run jshint && npm run jscs",
    "testrpc": "pm2 start testrpc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transmute-industries/transmute-framework.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "security",
    "javascript",
    "truffle",
    "transmute"
  ],
  "contributors": [
    "Eric Olszewski <eolszewski@gmail.com>",
    "Orie Steele <orie@or13.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transmute-industries/transmute-framework/issues"
  },
  "homepage": "https://github.com/transmute-industries/transmute-framework#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "ethereumjs-testrpc": "^3.0.5",
    "firebase": "^3.2.1",
    "firebase-tools": "^3.0.6",
    "http-server": "^0.9.0",
    "jscs": "^3.0.7",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.0",
    "truffle": "^3.2.1"
  }
}